<p id="notice"><%= notice %></p>

<h1>Expenses</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>Date</th>
      <th>Amount</th>
      <th>Payroll type</th>
      <th>Search engine</th>
      <th>Month</th>
      <th>Year</th>
      <th>Miscellaneous expense type</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expenses.each do |expense| %>
      <tr>
        <td><%= expense.company_name %></td>
        <td><%= expense.date %></td>
        <td><%= expense.amount %></td>
        <td><%= expense.payroll_type %></td>
        <td><%= expense.search_engine %></td>
        <td><%= expense.month %></td>
        <td><%= expense.year %></td>
        <td><%= expense.miscellaneous_expense_type %></td>
        <td><%= expense.company_id %></td>
        <td><%= link_to 'Show', expense %></td>
        <td><%= link_to 'Edit', edit_expense_path(expense) %></td>
        <td><%= link_to 'Destroy', expense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expense', new_expense_path %>
